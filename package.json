{
  "name": "kubi-web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"next dev\" \"tsx watch src/workers/event-listener.ts\" \"tsx watch src/workers/donation-queue-processor.ts\" \"tsx watch src/workers/rebase-scheduler.ts\"",
    "dev:next": "next dev",
    "dev:listener": "tsx watch src/workers/event-listener.ts",
    "dev:processor": "tsx watch src/workers/donation-queue-processor.ts",
    "dev:scheduler": "tsx watch src/workers/rebase-scheduler.ts",
    "build": "prisma generate && next build",
    "start": "concurrently \"next start\" \"tsx src/workers/event-listener.ts\" \"tsx src/workers/donation-queue-processor.ts\" \"tsx src/workers/rebase-scheduler.ts\"",
    "start:next": "next start",
    "start:listener": "tsx src/workers/event-listener.ts",
    "start:processor": "tsx src/workers/donation-queue-processor.ts",
    "start:scheduler": "tsx src/workers/rebase-scheduler.ts",
    "lint": "eslint",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:push": "prisma db push",
    "prisma:studio": "prisma studio",
    "seed:tokens": "npx tsx scripts/seed-tokens.ts",
    "seed:yield-tokens": "npx tsx scripts/seed-yield-tokens.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.911.0",
    "@prisma/client": "^6.17.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@rainbow-me/rainbowkit": "^2.2.8",
    "@tanstack/react-query": "^5.90.2",
    "@uniswap/widgets": "^2.59.0",
    "concurrently": "^9.2.1",
    "ethers": "^6.15.0",
    "iron-session": "^8.0.4",
    "lucide-react": "^0.545.0",
    "next": "15.5.9",
    "node-cron": "^4.2.1",
    "pusher": "^5.2.0",
    "pusher-js": "^8.4.0",
    "qrcode.react": "^4.2.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "siwe": "^3.0.0",
    "viem": "^2.37.13",
    "wagmi": "^2.17.5",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ws": "^8.18.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "prisma": "^6.17.0",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
